<div class="row py-3">
  <h3><span>メンバー一覧</span></h3>
</div>

<div class="row justify-content-center p-5">
  <div class="col-8 member-list">
    <table class="table">
      <% @members.each do |member| %>
      <tr>
        <td><%= link_to member.name, user_path(member) %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>